<view class="main"><view class="swiperCont"><swiper class="swiper" indicator-dots="true" autoplay="true" interval="3000" duration="500" circular="true" indicator-active-color="#ffffff" indicator-color="rgba(255, 255, 255, .6)"><block wx:for="{{banners}}" wx:for-item="item" wx:for-index="index"><block wx:if="{{item.typeTitle=='新歌首发'}}"><swiper-item><navigator class="swiper-item" url="{{'/pages/musicDetail/musicDetail?id='+item.targetId}}"><image src="{{item.pic}}"></image></navigator></swiper-item></block></block></swiper></view><view class="editorRecommend"><view class="title">编辑推荐</view><view class="songList_wrap"><block wx:for="{{$root.l0}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="songList_item" url="{{'/pages/playList/playList?playListId='+item.$orig.id}}" open-type="navigate"><view class="songList_img"><image src="{{item.$orig.picUrl}}" mode="widthFix"></image><view class="play_count_wrap"><view class="play_count_img"><image src="https://i.niupic.com/images/2021/09/09/9uS3.png" mode="widthFix"></image></view><view class="play_count">{{item.f0}}</view></view></view><view class="songList_title">{{item.$orig.name}}</view></navigator></block></view></view><view class="newSongs"><view class="newSongs_title">最新音乐</view><view class="newSongs_wrap"><block wx:for="{{newSongList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><navigator class="newSongs_item" url="{{'/pages/musicDetail/musicDetail?id='+item.id}}"><view class="newSongs_item_title"><view class="newSongs_name">{{item.name}}</view><view class="artist"><view class="artist_img_item"><view class="artist_img"></view></view><view class="artist_name">{{item.song.artists[0].name+'-'+item.song.album.name}}</view></view></view><view class="newSongs_img"></view></navigator></block></view></view><navigator class="footer"><view class="footer_img"><image src="https://i.niupic.com/images/2021/09/09/9uRG.png" mode="widthFix"></image></view><view class="openapp">打开APP，发现更多好音乐</view><view class="copyright"><text>网易公司版权所有©1997-2021杭州乐读科技有限公司运营</text></view></navigator></view>